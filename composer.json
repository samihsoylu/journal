{
    "name": "samihsoylu/journal",
    "description": "Document your life, daily happenings, special occasions, and reflections on your goals.",
    "type": "project",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "SamihSoylu\\Journal\\Framework\\": "framework/",
            "SamihSoylu\\Journal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SamihSoylu\\Journal\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Samih Soylu",
            "email": "mail@samihsoylu.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "ext-openssl": "*",
        "doctrine/orm": "^2.16",
        "doctrine/dbal": "^3.6",
        "doctrine/migrations": "^3.6",
        "ramsey/uuid-doctrine": "^2.0",
        "adrenalinkin/doctrine-naming-strategy": "^2.0",
        "samihsoylu/cipher-suite-php": "^2.0",
        "samihsoylu/utility-php": "^1.0",
        "symfony/finder": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5",
        "psy/psysh": "^0.12.2"
    },
    "require-dev": {
        "pestphp/pest": "^2.16",
        "symfony/var-dumper": "^7.0",
        "mockery/mockery": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.34",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.16",
        "rector/rector": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
