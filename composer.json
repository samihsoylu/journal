{
    "name": "samihsoylu/journal",
    "description": "Document your life, daily happenings, special occasions, and reflections on your goals.",
    "type": "project",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Samih Soylu",
            "email": "mail@samihsoylu.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "private/lib/",
            "Tests\\": "tests/lib/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "doctrine/orm": "^2.7",
        "doctrine/annotations": "^1.13",
        "vlucas/phpdotenv": "^5.4",
        "jenssegers/blade": "^1.3",
        "nikic/fast-route": "^1.3",
        "symfony/cache": "^5.0",
        "psr/cache": "^1.0",
        "erusev/parsedown": "^1.7",
        "defuse/php-encryption": "^2.2",
        "doctrine/migrations": "^3.1",
        "psy/psysh": "^0.10.8",
        "symfony/console": "^5.2",
        "sentry/sdk": "^3.1",
        "symfony/polyfill-php80": "^1.26",
        "symfony/polyfill-php81": "^1.26"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9",
        "php-parallel-lint/php-parallel-lint": "dev-master",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/phpstan": "^1.3",
        "phpcompatibility/php-compatibility": "^9.3"
    }
}
